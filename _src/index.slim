		javascript:
			let html = 	document.querySelector('html');
			let meta = document.querySelector('meta[name="theme-color"]');
			let toggle = document.querySelector('.dark-toggle');
			let comet = document.querySelector('.comet');
			let now = new Date();
			let hours = now.getHours();

			toggle.addEventListener("click", toggleDark);

			function toggleDark() {
				html.classList.toggle('dark');

				if (!html.classList.contains('dark')) {
					// Turn on Dark Mode
					meta.setAttribute("content", "#ffffff");
					toggle.innerHTML = "Turn on <strong>Dark Mode</strong>";
					localStorage.removeItem("bennettfeely-dark-mode");
				} else {
					// Turn off Dark Mode
					meta.setAttribute("content", "#16161d");
					toggle.innerHTML = "Turn off <strong>Dark Mode</strong>";
					localStorage.setItem("bennettfeely-dark-mode", "enabled");
				}
			}
			window.dataLayer = window.dataLayer || [];
			function gtag(){dataLayer.push(arguments);}
			gtag('js', new Date());
			gtag('config', 'UA-35724550-1');

		